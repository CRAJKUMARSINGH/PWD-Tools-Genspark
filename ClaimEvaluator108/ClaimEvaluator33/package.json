    "start:one-click": "npm install && npm run build && npm start",
    "start:replit": "npm run dev:replit",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "node tests/unit-tests.js",
    "test:all": "node tests/test-runner.js",
    "test:ci": "cross-env CI=true node tests/test-runner.js",
    "test:performance": "node tests/performance-tests.js",
    "test:web": "node tests/web-tests.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rimraf dist node_modules/.cache .replit-cache",
    "optimize-bundle": "node scripts/optimize-bundle.js",
    "deploy:vercel": "vercel --prod",
    "deploy:one-click": "npm run build:optimize && npm run deploy:vercel",
    "deploy:full": "node scripts/one-click-deploy.js",
    "deploy:replit": "npm install && npm run build",
    "quantum:calculate": "node scripts/quantum-calculations.js",
    "claims:analyze": "node scripts/claims-analyzer.js",
    "docs:generate": "node scripts/generate-docs.js",
